<script type="text/javascript" src="https://cdn-prod.mybharats.in/mybharat/js/jquery.min.js"></script> 
<script>
    var cdn_url = 'https://cdn-prod.mybharats.in/'; 
    var cdn_path = 'https://cdn-prod.mybharats.in/mybharat/';
    var allowedOrgIds = 'Array';//get data from config
    var language_id  = 'en';
    var tasksData  = [];
    var slugdata  = 'govt-girls-polytechnic-dehradun';
    var uriSegments = window.location.pathname.split('/');
    var category = uriSegments[2].replace(/-/g, "_");
    var stateid = uriSegments[3];
    function readInternshipData(partnerProfile){
        $.getJSON(cdn_url + "yuva_json/knowledgeInstitutions_data/" + category.toLowerCase() + "/" +partnerProfile[0]['org_id'] + "/elos.json", function() {
            }).done(function(data){               
            setTimeout(function(){
                setInternshipData(data.elos.data);
            },100);
        })
        .fail(function() { 
            setInternshipData([]);
        });
    }
    function setInternshipData(data){
        tasksData = data;
    }
   
    function readEventsData(partnerProfile){
        $.getJSON(cdn_url + "yuva_json/knowledgeInstitutions_data/" + category.toLowerCase() +  "/" + partnerProfile[0]['org_id'] + "/events.json", function() {  
            }).done(function(data){
             setTimeout(function(){
                setOrgEventsData(data.events.data,partnerProfile);
            },200);
        }).fail(function() { 
            setOrgEventsData([],partnerProfile);
        
        });
    }
    function setOrgEventsData(eventsData,partnerProfile){        
        var partnerData = [];
        partnerData["event"] = [];
        partnerData["eventCount"]="";
        partnerData["internship"] = [];
        level_data_array = [];
        var eventSpecializationIds = [];
        var commonValues = [];
        let loopEvent = false;
        let loopInternship = false;
        if(partnerProfile && partnerProfile.length !=0){
            $.each(eventsData, function(key,val){
                if (loopEvent) {
                    return false;
                }
                //perticulr org event
                if(allowedOrgIds.includes(partnerProfile[0]['org_id'])){
                    if(val['specialization']){
                        eventSpecializationIds = val['specialization'].split(',');
                    }
                    if(eventSpecializationIds && partnerProfile[0]['specialization']){
                        commonValues = eventSpecializationIds.filter(function(n) {
                                return partnerProfile[0]['specialization'].indexOf(n) !== -1;
                        });
                    }
                    if(commonValues){
                        partnerData["event"].push(val);
                        if (partnerData["event"].length > 2) {
                            loopEvent = true;
                        }
                    }
                }else{
                    partnerData["event"].push(val);
                }
            });
            $.each(tasksData, function(ky,value){
                if (loopInternship) {
                    return false;
                }
                partnerData["internship"].push(value);
            });

            partnerData["eventCount"] = partnerData["event"].length;
            partnerData["internshipCount"] = partnerData["internship"].length;

            partnerData["event"] = partnerData["event"].slice(0,3);
            partnerData["internship"] = partnerData["internship"].slice(0,3);
            level_data_array = partnerProfile[0]['level_data'];
            buildPages(partnerProfile[0],partnerData,level_data_array);
        }else{
            buildPages(partnerProfile,partnerData,level_data_array); ///now added
        }
    }
    function buildPages(partnerProfile,partnerData,level_data_array){
        var imagePath ="";
        var banner_image ="";
        var output = '';//level_box
        var hierarchy_image = 'https://cdn-prod.mybharats.in/mybharat/assets/img/yuva_landing/Default-default.png';
        var hierarchy_link ='';
        var view_more_link ='';
        var hierarchy_data_link ='';
        var category = 'polytechnic';
        var state_id = '';
        var volunteerData ='';
        var $rand_num = Math.floor(Math.random()*(999-100+1)+100);
        var $last_date = '';
        var internshipData ='';
        
        if(partnerProfile['org_photo']){
            imagePath = cdn_url+'organization/'+partnerProfile['org_photo'];
        }else{
            imagePath = cdn_path+'/assets/img/yuva_landing/img_not_available_v2_icon.jpg';
        }
        if(partnerProfile['org_banner']){
            banner_image = cdn_url+'organization/'+partnerProfile['org_banner'];
        }else{
            banner_image = cdn_path+'/assets/img/yuva_landing/img_not_available_v2_icon.jpg';
        }
        $('.pro_a1 h4').text(partnerProfile.org_name);
        $('#org_about').html(partnerProfile.org_about);
        $("#partnerimage1").attr("src",banner_image);
        $("#imageContainer1 img").attr("src",imagePath);

        var volunteer_view_more_link ='';
        //volunteer view more link
        if(partnerData['event'] && partnerData['eventCount'] > 3){
            volunteer_view_more_link += '<a href="/KnowledgeInstitutions/'+category+ '/' +stateid+ '/' +partnerProfile.slug+'/events'+'"><b>View More('+partnerData['eventCount']+')</b></a>';
        }
        $('.volunteer_view_more').append(volunteer_view_more_link);
        // Volunteer For Bharat card start
        volunteerData += '<div id="home1" class="" style="padding:0px;">';
        volunteerData += '<div class="wrapper" id="wrapper3">';
        if(partnerData['event'] === "" || partnerData['event'].length === 0){//if no event
            volunteerData += '<img src="'+cdn_path+'assets/img/yuva_landing/ComingSoonELO.png" class="no_img">';
        }
        if (partnerData['event'] && partnerData['event'].length>3 && partnerData.eventCount>3) {
            volunteerData +='<div class="icon"><i id="left" class="fa fa-angle-left"></i></div>';
        }
        volunteerData += '<ul class="tabs-box">';
        if (partnerData['event']){
            $.each(partnerData['event'], function(pkey, event){
                if (event.last_rsvp_date != undefined) {
                    last_date = formatToReadableDate(event.last_rsvp_date);
                }
                if (event.event_from_date != undefined) {
                    var $formated_event_from_date = event.event_from_date.split(' ');
                    var $month = formatToReadableDate(event.event_from_date,'month');
                    var $to_month = formatToReadableDate(event.event_to_date,'month');
                    var $from_day = formatToReadableDate(event.event_from_date,'day');
                    var $to_day = formatToReadableDate(event.event_to_date,'day');
                    $month = ($month === $to_month) ? $month : $month + ' ' + $to_month;
                    $date = ($month === $to_month && $from_day === $to_day) ? $from_day : $from_day + '  -  ' + $to_day;
                    $mobileDate = ($month === $to_month && $from_day === $to_day) ? $to_day +' ' + $month : $to_day + ' ' + $month + '  -  ' +$from_day + ' ' + $to_month;
                }
                var event_name = event.event_name.replace(' ', '-');
                var district_name = '';
                var state_name = '';
                if (event.district != '' && event.district != null) {
                    district_name = event.district + ',';
                }
                if (event.state != '' && event.state != null) {
                    state_name = event.state;
                }
                var event_rand_id = $rand_num +''+ event['id'] +''+ $rand_num;
                volunteerData += '<a href="/pages/event_detail?event_name='+event_name+'&key='+event_rand_id+'">';

                volunteerData += '<li class="tab">';
                volunteerData += '<div class="card border-0">';
                volunteerData += '<div class="card-header p-0 position-relative">';                
                    if(event['image_path']){
                        eventimage = cdn_url+'events/'+event['image_path'];
                    }else{
                        eventimage = cdn_path+'/assets/img/yuva_landing/img_not_available_v2_icon.jpg';
                    }
                volunteerData += '<img src="'+eventimage+'" class="eventimage" alt="event image" style="width: 350px;height: 250px;">';
                if(event['getnyfeventcomponent']){                      
                    event['getnyfeventcomponent'].forEach(function (nyfcomponent) {
                        volunteerData += '<div class="top-right1">';   
                        volunteerData += '<p>'+nyfcomponent+'</p>';
                        volunteerData += '</div>';
                    });
                }
                volunteerData += '<div class="bottom-left1">';
                if(event['last_rsvp_date']){
                    volunteerData += 'Last date to apply:'+ last_date;
                }
                volunteerData += '</div>';
                volunteerData += '</div>';                
                volunteerData += '<div class="card-body">';
                volunteerData += '<table><tr>';
                volunteerData += '<td>';
                volunteerData += '<div class="card-bottom-left">';
                volunteerData += '<div>';
                volunteerData += '<div class="dt_a1">'+$month;
                volunteerData += '</div>';
                volunteerData += '<div class="dt_a2">'+$date;
                volunteerData += '</div>';
                volunteerData += '</div>';
                volunteerData += '<div class="card-bottom-divider"></div>';
                volunteerData += '</div>';
                volunteerData += '</td>';
                volunteerData += '<td><span class="mdt_b1">'+event['event_name']+'</span><br>';               
                if(state_name !='' && state_name != null){ 
                    volunteerData += '<span class="mdt_b2"><i class="fa fa-map-marker" style="color:#F15B43" aria-hidden="true"></i>'+district_name+ ' ' + state_name+'</span>';
                }
                volunteerData += '</td>';
                volunteerData += '</tr></table>';
                volunteerData += '</div>';//end card-body
                volunteerData += '</div>';
                volunteerData += '</li>';
                volunteerData += '</a>'
            });
            if(partnerData['event'].length > 2){ 
                volunteerData += '<a href="/KnowledgeInstitutions/'+category+ '/' +stateid+ '/' +partnerProfile.slug+'/events'+'">';
                volunteerData += '<li>';
                volunteerData += '<div class="border-0">'
                volunteerData += '<div class="show_more">';
                volunteerData += '<img src="'+cdn_path+'assets/img/yuva_landing/show-more-2x.png" alt="show more"/>';
                volunteerData += '</div>';
                volunteerData += '</div>';
                volunteerData += '</li>';
                volunteerData += '</a>';
            }
        }
        volunteerData += '</ul>';
        if (partnerData['event'] && partnerData['event'].length>2) {
            volunteerData += '<div class="icon"><i id="right" class="fa fa-angle-right"></i></div>';
        }
        volunteerData += '</div>';//end wrapper3
        volunteerData += '</div>';
        $('.eventcard').html(volunteerData);
        
        var internship_view_more_link='';
        //internship view more link
        if(partnerData['internship'] && partnerData['internshipCount'] > 3){
            internship_view_more_link += '<a href="/KnowledgeInstitutions/'+category+'/'+stateid+'/'+partnerProfile.slug+'/experiential_learning'+'"><b>View More('+partnerData['internshipCount']+')</b></a>';
        }
        $('.internship_view_more_link').append(internship_view_more_link);
        
        //append intershipdata
        internshipData += '<div class="" style="margin-bottom: 0px;">';
        internshipData += '<div id="" class="" style="padding:0px;">';
        internshipData += '<div class="wrapper" id="wrapper1">';
        if(partnerData['internship'] === "" || partnerData['internship'].length === 0){//if no event
            internshipData += '<img src="'+cdn_path+'assets/img/yuva_landing/ComingSoonELO.png" class="no_img">';
        }
        if(partnerData['internship'] && partnerData['internship'].length>2){
            internshipData += '<div class="icon"><i id="left" class="fa fa-angle-left"></i></div>';
        }
        internshipData += '<ul class="tabs-box">';
        if(partnerData['internship']){
            $.each(partnerData['internship'], function(ikey, internship){   
                var task_name = internship.task_name.replace(' ', '-'); 
                var internship_rand_id = $rand_num +''+ internship.id +''+ $rand_num;                
                var image_name = internship.img_path1.split('/').pop();
                var internship_img_path = (internship.img_path1 != '' && internship.img_path1 != null) ? cdn_url+'tasks/'+image_name : cdn_path+'/assets/img/yuva_landing/img_not_available_v2_icon.jpg';
                var internship_last_date = '';
                if (internship.interest_date != undefined) {
                    internship_last_date = formatToReadableDate(internship.interest_date);
                }
                if (internship.start_date != undefined) {
                    var $formated_start_date = internship.start_date.split(' ');
                    var internship_month = formatToReadableDate(internship.start_date,'month');
                    var $to_month = formatToReadableDate(internship.end_date,'month');
                    var $from_day = formatToReadableDate(internship.start_date,'day');
                    var $to_day = formatToReadableDate(internship.end_date,'day');
                    internship_month = (internship_month === $to_month) ? internship_month : internship_month + ' ' + $to_month;
                    internship_date = (internship_month === $to_month && $from_day === $to_day) ? $from_day : $from_day + '  -  ' + $to_day;
                    $mobileDate = (internship_month === $to_month && $from_day === $to_day) ? $to_day +' ' + internship_month : internship_month + ' ' + $to_day + '  to  ' +$to_month + ' ' + $from_day;
                }
                var internship_city_name = '';
                var internship_state_name = '';
                if (internship.city_name != '' && internship.city_name != null) {
                    internship_city_name = internship.city_name + ',';
                }
                if (internship.state_name != '' && internship.state_name != null) {
                    internship_state_name = internship.state_name;
                }

                internshipData += '<a href="/pages/experiential_learning_detail?task_name='+task_name+'&key='+internship_rand_id+'">';
                internshipData += '<li class="tab">';
                internshipData += '<div class="card border-0">';
                internshipData += '<div class="card-header p-0 position-relative">';
                internshipData += '<img src="'+internship_img_path+'" class="internimage" alt="internship image" style="width: 350px;height: 250px;">';
                if(internship['org_type']){
                    internshipData += '<div class="top-right1">'+internship['org_type']+'</div>';
                }
                if(internship_last_date != '' && internship_last_date != null){
                    internshipData += '<div class="bottom-left1">';
                    internshipData += 'Last date to Apply: '+internship_last_date;
                    internshipData += '</div>';
                }
                internshipData += '</div>';//card-header p-0 position-relative
                internshipData += '<div class="card-body">';
                internshipData += '<table><tr>';
                internshipData += '<td>';
                internshipData += '<div class="card-bottom-left">';
                internshipData += '<div>';
                internshipData += '<div class="dt_a1">'+internship_month;
                internshipData += '</div>';
                internshipData += '<div class="dt_a2">'+internship_date;
                internshipData += '</div>';
                internshipData += '<div style="text-align: center;">'
                internshipData += '<span class="dt_a1 dt_a1_beta">OPENINGS</span><br><span class="dt_a2">'+internship['no_of_opening']+'</span>';
                internshipData += '</div>';
                internshipData += '</div>';  
                internshipData += '<div class="card-bottom-divider"></div>';
                internshipData += '</div>'; 
                internshipData += '</td>';

                internshipData += '<td>';

                internshipData += '<span class="mdt_b1">'+internship['task_name']+'</span><br>';
                internshipData += '<span class="mdt_b2"><i class="fa fa-map-marker" style="color:#F15B43" aria-hidden="true"></i>';
                internshipData += internship_city_name+ ' ' +internship_state_name;
                internshipData += '</span><br>';
                internshipData += '<span class="mdt_b2"><i class="fa fa-clock" style="color:#F15B43" aria-hidden="true"></i>'+internship['credit_hour']+' Hours</span>'
                internshipData += '</td>';
                internshipData += '</tr></table>';
                internshipData += '</div>';//end card-body
                internshipData += '</div>';//end card border-0
                internshipData += '</li>';
                internshipData += '</a>';
            });
            if(partnerData['internship'].length > 2){ 
                internshipData += '<a href="/KnowledgeInstitutions/'+category+'/'+stateid+'/'+partnerProfile.slug+'/experiential_learning'+'">';
                internshipData += '<li>';
                internshipData += '<div class="border-0">'
                internshipData += '<div class="show_more">';
                internshipData += '<img src="'+cdn_path+'assets/img/yuva_landing/show-more-2x.png" alt="show more"/>';
                internshipData += '</div>';
                internshipData += '</div>';
                internshipData += '</li>';
                internshipData += '</a>';
            }
        }
        internshipData += '</ul>';
        if (partnerData['internship'] && partnerData['internship'].length>2) {
            internshipData += '<div class="icon"><i id="right" class="fa fa-angle-right"></i></div>';
        }
        internshipData += '</div>';
        internshipData += '</div>';
        internshipData += '</div>';      
        $('.internshipcard').append(internshipData);
        initializeSlider("wrapper3", 300);
        initializeSlider("wrapper1", 300);
    }
    function getPartnerProfile(){
        var uriSegments = window.location.pathname.split('/');
        var category = uriSegments[2].replace(/-/g, "_");
        var stateid = uriSegments[3];
        var partnerProfile = [];
        $.getJSON(cdn_url+"yuva_json/knowledgeInstitutions_data/" + category.toLowerCase() +"/"+stateid+ ".json", function(response) {
            }).done(function(data) {
                setTimeout(function(){
                    getPartnerData(data[stateid]['data']);
                 },100);
        })
        
    }
    function getPartnerData(partnerProfile){
        var partner_array = [];
        if(partnerProfile){
            $.each(partnerProfile,function (key,profile) {
                if (profile.slug == slugdata) {
                    partner_array.push(profile);
                }
            });
            if(partner_array == ''){
                window.location.href = '/pages/pagenot_found';
            }else{
                setInternshipData(partner_array[0].elos.data);
                setOrgEventsData(partner_array[0].events.data,partner_array);
                setClasses(partner_array[0].courses,partner_array[0]); 
            } 
        }
    }
    function setClasses(classes,partner_array){
        var size = Object.keys(classes).length;
        if(size > 0){
            var output = '';
            output += '<div class="row">';
            output += '<h3 class="about-org">Classes('+size+')</h3>';
            $.each(classes,function(key,val){
                output += '<div class="col-md-4">';
                output += '<a href="/KnowledgeInstitutions/'+category+'/'+stateid+'/'+partner_array.slug+'/class/'+key+'"><div class="org-class">'+key+'</div></a>';
                output += '</div>';
            })
            output += '</div>';
            $('#classes').html(output);
        }

    }
    function formatToReadableDate(date,type = 'date') {
        var monthArr =[
                        'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'
                    ];
        var d = new Date(date),
            month = '' + monthArr[d.getMonth()];
            day = '' + (d.getDate()),
            year = d.getFullYear();
        if (day.length < 2) 
            day = '0' + day;
            var returnDate = day+' '+month+ ', '+year;
        if(type == 'date'){
            var returnDate = day+' '+month+ ', '+year;
        }else if(type == 'month'){
            var returnDate = month;
        }else if(type == 'day'){
            var returnDate = day;
        }
        return returnDate;
    }
    $(document).ready(function () { 
        getPartnerProfile();
       
    });

    function initializeSlider(wrapperId, scrollWidth = 340) {
    const tabsBox = document.getElementById(wrapperId).querySelector(".tabs-box");
    const arrowIcons = document.getElementById(wrapperId).querySelectorAll(".icon i");
    let isDragging = false;
    let touchStartX = 0;

    const handleIcons = () => {
        let maxScrollableWidth = tabsBox.scrollWidth - tabsBox.clientWidth;
        const isMobile = window.innerWidth <= 767;

        arrowIcons[0].parentElement.style.display = tabsBox.scrollLeft <= 0 || isMobile ? "none" : "flex";
        arrowIcons[1].parentElement.style.display =
            maxScrollableWidth - tabsBox.scrollLeft <= 1 || isMobile ? "none" : "flex";
    };

    arrowIcons.forEach((icon) => {
        icon.addEventListener("click", () => {
            tabsBox.scrollLeft += icon.id === "left" ? -scrollWidth : scrollWidth;
            handleIcons();
        });
    });

    const startDragging = (e) => {
        isDragging = true;
        tabsBox.classList.add("dragging");
        touchStartX = e.touches ? e.touches[0].clientX : e.clientX;

        // Check if the target is not an anchor (<a>) or its parent
        if (!isAnchorOrChildOfAnchor(e.target)) {
            e.preventDefault();
        }
    };

    const dragging = (e) => {
        if (!isDragging) return;
        const touchX = e.touches ? e.touches[0].clientX : e.clientX;
        tabsBox.scrollLeft -= touchX - touchStartX;
        touchStartX = touchX;
        handleIcons();
        e.preventDefault(); // Prevent default only when dragging over non-anchor elements
    };

    // Helper function to check if the element or its parent is an anchor (<a>)
    const isAnchorOrChildOfAnchor = (element) => {
        return element.tagName.toLowerCase() === "a" || element.closest("a") !== null;
    };

    const dragStop = () => {
        isDragging = false;
        tabsBox.classList.remove("dragging");
    };

    tabsBox.addEventListener("mousedown", startDragging);
    tabsBox.addEventListener("touchstart", startDragging);

    tabsBox.addEventListener("mousemove", dragging);
    tabsBox.addEventListener("touchmove", dragging);

    document.addEventListener("mouseup", dragStop);
    document.addEventListener("touchend", dragStop);

    // Additional handling for touch cancel event
    tabsBox.addEventListener("touchcancel", () => {
        isDragging = false;
        tabsBox.classList.remove("dragging");
    });

    // Additional handling for touch leave event (in case touchcancel is not triggered)
    tabsBox.addEventListener("touchleave", () => {
        isDragging = false;
        tabsBox.classList.remove("dragging");
    });

    // Additional handling for touch move outside the element (in case touchcancel is not triggered)
    tabsBox.addEventListener("touchmove", (e) => {
        const touchX = e.touches ? e.touches[0].clientX : e.clientX;
        if (touchX < tabsBox.getBoundingClientRect().left || touchX > tabsBox.getBoundingClientRect().right) {
            isDragging = false;
            tabsBox.classList.remove("dragging");
        }
    });

    // Inside your anchor click event handler, you may need to remove the preventDefault
    const handleAnchorClick = (e) => {
        if (tabsBox.classList.contains("dragging")) {
            // Do not prevent default if dragging is in progress
            // You may also add additional logic or actions here if needed
        }
    };

    // Find the anchor tag inside the li element and attach the click event handler
    const anchorTag = document.querySelector(`#${wrapperId} .tab a`);
    if (anchorTag) {
        anchorTag.addEventListener("click", handleAnchorClick);
    }
}

</script>

