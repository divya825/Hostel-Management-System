 <script>
	function copyShareLink() {
		const url = 'https://mybharat.gov.in/KnowledgeInstitutions/polytechnic/34/govt-girls-polytechnic-dehradun';
		$('<textarea>').val(url).appendTo('body').select();
		document.execCommand('copy');
      alert('Link copied to clipboard!');
      // showSuccess('#success', 'Link copied to clipboard!');
      $('textarea').remove();
	}

	// Utils 
	// Displays success message with auto-hide and scroll to message
	function showSuccess(element, message){
		$(element).text(message)
		$(element).removeClass('hidden');
		window.scrollTo({ top: 0, left: 0, behavior: 'smooth' });
		setTimeout(()=> $(element).addClass('hidden'), 5000);
	}

	// Displays error message with auto-hide and scroll to message
	function showError(element, message){
		$(element).text(message)
		$(element).removeClass('hidden');
		window.scrollTo({ top: 0, left: 0, behavior: 'smooth' });
		setTimeout(()=> $(element).addClass('hidden'), 5000);
	}
</script>                                <!-- for social media end -->
